<Window x:Class="_3ulerBotServer.ModifyChannel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3ulerBotServer.ViewModel"
        mc:Ignorable="d"
        Title="ModifyChannel" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="{Binding ChannelBeingModified.Name}"></TextBlock>
        <TextBlock Text="{Binding ChannelBeingModified.ChannelId}"></TextBlock>
        <ListBox ItemsSource="{Binding ChannelBeingModified.ChannelCommands}">
            <ListBox.ItemTemplate>
                <DataTemplate>
                    <StackPanel>
                        <TextBlock Text="{Binding CommandString}"></TextBlock>
                    </StackPanel>
                    </DataTemplate>
                </ListBox.ItemTemplate>
        </ListBox>
        <Button Content="Close" Name="CloseChannelDialog" Click="CloseChannelDialog_OnClick"></Button>
    </StackPanel>
</Window>
